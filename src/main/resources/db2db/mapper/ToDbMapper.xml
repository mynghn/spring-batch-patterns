<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mynghn.springbatchpatterns.db2db.mapper.ToDbMapper">
  <delete id="cleanUp" parameterType="java.util.Map">
    DELETE FROM PUBLIC.COPIED_FROM_EXTERNAL_SOURCE
    WHERE BASE_DATE = #{baseDate}
  </delete>
</mapper>
